{
  "name": "generate-defaults",
  "description": "Use this generator to add some common default settings to your 'generate', verb, and assemble generators.",
  "version": "0.1.2",
  "homepage": "https://github.com/jonschlinkert/generate-defaults",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/generate-defaults",
  "bugs": {
    "url": "https://github.com/jonschlinkert/generate-defaults/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "common-middleware": "^0.2.5",
    "empty-dir": "^0.2.0",
    "engine-base": "^0.1.2",
    "generate-collections": "^0.1.1",
    "helper-date": "^0.2.2",
    "isobject": "^2.0.0",
    "lazy-cache": "^1.0.3",
    "mixin-deep": "^1.1.3",
    "omit-empty": "^0.3.2",
    "parse-author": "^0.2.0",
    "parse-github-url": "^0.3.0"
  },
  "devDependencies": {
    "generate": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "*",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "mocha": "*"
  },
  "keywords": [
    "generate",
    "generator",
    "template",
    "templates",
    "verb",
    "verb-generator",
    "verbgenerator"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "generate",
      "list": [
        "assemble",
        "generate",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "common-middleware",
      "generate",
      "verb",
      "generate-collections"
    ],
    "lint": {
      "reflinks": true
    }
  }
}