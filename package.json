{
  "name": "generate-defaults",
  "description": "Use this generator to add some common default settings to your 'generate', verb, and assemble generators.",
  "version": "0.2.1",
  "homepage": "https://github.com/generate/generate-defaults",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-defaults",
  "bugs": {
    "url": "https://github.com/generate/generate-defaults/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "verb-defaults": "^0.1.0",
    "verb-repo-data": "^0.1.1"
  },
  "devDependencies": {
    "generate": "^0.5.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.8",
    "gulp-istanbul": "^0.10.4",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "generate",
    "generator",
    "template",
    "templates",
    "verb",
    "verb-generator",
    "verbgenerator"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "generate",
      "list": [
        "assemble",
        "generate",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "common-middleware",
      "generate",
      "generate-collections",
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
